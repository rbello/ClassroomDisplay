RewriteEngine On
RewriteRule	^phpmyadmin$	/phpMyAdmin/index.php [NC,L]
RewriteRule	^phpmyadmin/$	/phpMyAdmin/index.php [NC,L]

RewriteRule	^([a-zA-Z\s]+)/([0-9]+)/([0-9]+)/?$	index.php?site=$1&date=$2&afficheur=$3 [NC,L]
RewriteRule	^([a-zA-Z\s]+)/([0-9]+)/([0-9]+)$	index.php?site=$1&date=$2&afficheur=$3 [NC,L]

RewriteRule	^([a-zA-Z\s]+)/([0-9]+)/?$	index.php?site=$1&date=$2 [NC,L]
RewriteRule	^([a-zA-Z\s]+)/([0-9]+)$	index.php?site=$1&date=$2 [NC,L]

RewriteRule	^([a-zA-Z\s]+)/?$	index.php?site=$1 [NC,L]
RewriteRule	^([a-zA-Z\s]+)$	index.php?site=$1 [NC,L]

RewriteRule	^admin/([a-zA-Z]+)/$	admin.php?a=$1 [NC,L]

RewriteRule	^admin$	admin.php [NC]
RewriteRule	^admin/$	admin.php [NC]

